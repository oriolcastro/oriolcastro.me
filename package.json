{
  "name": "oriolcastro.me",
  "version": "3.0.0",
  "private": true,
  "description": "My personal blog",
  "keywords": [
    "gatsby"
  ],
  "homepage": "https://oriolcastro.me",
  "license": "MIT",
  "author": "Oriol Castro",
  "main": "n/a",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "lint": "eslint .",
    "lint:js": "NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .js,.jsx,.html --report-unused-disable-directives",
    "lint:mdx": "NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .mdx --report-unused-disable-directives",
    "lint:package": "sort-package-json",
    "preload-fonts": "gatsby-preload-fonts",
    "serve": "gatsby serve",
    "start": "npm run develop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "content/**/*.{mdx}": [
      "npm run lint:mdx --fix"
    ],
    "package.json": [
      "npm run lint:package"
    ],
    "src/**/*.{js,jsx}": [
      "npm run lint:js --fix"
    ],
    "src/**/*.{mdx}": [
      "npm run lint:mdx --fix"
    ]
  },
  "browserslist": ">0.5%, not ie 11, not safari 5.1, not op_mini all",
  "dependencies": {
    "@mdx-js/mdx": "1.6.18",
    "@mdx-js/react": "1.6.18",
    "@theme-ui/typography": "0.3.0",
    "dotenv": "8.2.0",
    "fontsource-merriweather": "3.0.9",
    "fontsource-open-sans": "3.0.9",
    "gatsby": "2.24.78",
    "gatsby-image": "2.4.21",
    "gatsby-plugin-alias-imports": "1.0.5",
    "gatsby-plugin-catch-links": "2.3.15",
    "gatsby-plugin-google-analytics": "2.3.18",
    "gatsby-plugin-manifest": "2.4.34",
    "gatsby-plugin-mdx": "1.2.46",
    "gatsby-plugin-mdx-embed": "0.0.14",
    "gatsby-plugin-netlify": "2.3.19",
    "gatsby-plugin-offline": "3.2.32",
    "gatsby-plugin-preconnect": "^1.1.54",
    "gatsby-plugin-preload-fonts": "^1.2.28",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-robots-txt": "1.5.3",
    "gatsby-plugin-sharp": "2.6.42",
    "gatsby-plugin-theme-ui": "0.3.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "3.3.34",
    "gatsby-source-filesystem": "^2.3.34",
    "gatsby-source-graphql": "2.7.6",
    "gatsby-transformer-remark": "2.8.40",
    "gatsby-transformer-sharp": "2.5.17",
    "lodash": "4.17.20",
    "mdx-embed": "0.0.14",
    "polished": "4.0.2",
    "prism-react-renderer": "1.1.1",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-helmet": "6.1.0",
    "react-icons": "3.11.0",
    "react-live": "2.2.2",
    "theme-ui": "0.3.1",
    "typography-theme-sutro": "0.16.19"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-import-resolver-custom-alias": "^1.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-mdx": "^1.8.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "remark-preset-prettier": "^0.4.0",
    "sort-package-json": "^1.46.0"
  }
}
