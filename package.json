{
  "name": "oriolcastro.me",
  "version": "3.0.0",
  "private": true,
  "description": "My personal blog",
  "keywords": [
    "gatsby"
  ],
  "homepage": "https://oriolcastro.me",
  "license": "MIT",
  "author": "Oriol Castro",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "lint": "eslint .",
    "lint:js": "NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .js,.jsx,.html,.ts,.tsx,.mjs --report-unused-disable-directives",
    "lint:package": "sort-package-json",
    "serve": "gatsby serve",
    "start": "npm run develop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "npm run lint:package"
    ],
    "src/**/*.{js,jsx}": [
      "npm run lint:js --fix"
    ]
  },
  "browserslist": ">0.5%, not ie 11, not safari 5.1, not op_mini all",
